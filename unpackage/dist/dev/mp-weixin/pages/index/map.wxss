
.map_style{
	width: 100%;
	height: 85vh;
}
.foot {
    width: 100%;
    height: 90px;
    position: fixed;
    bottom: 0;
    left: 0;
}
#button {
    width: calc(100% - 20px);
    height: calc(100% - 40px);
    padding: 10px;
    margin: 10px;
    background-color: #295dca;
	color: white;
    border: none;
    border-radius: 30px;
    line-height: 30px;
    font-size: large;
    font-family: "\6977\4F53", serif;
}
#label {
    margin: 10px;
}

